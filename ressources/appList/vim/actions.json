{
  "name": "Vim",
  "entries": [
    {
      "action": "ciw",
      "description": "Cut inner word."
    },
    {
      "action": "ci\"",
      "description": "Cut inside quotes."
    },
    {
      "action": ":%s/foo/bar/g",
      "description": "Find and replace in all lines."
    },
    {
      "action": ":s/foo/bar/g",
      "description": "Find and replace in current line."
    },
    {
      "action": ":s/\\<foo\\>/bar/gc",
      "description": "Find and replace only whole words maching exactly \"foo\" in current line with confirmation."
    },
    {
      "action": ":.,+2s/foo/bar/g",
      "description": "Find and replace from the current line and the next 2 lines."
    },
    {
      "action": ":wa",
      "description": "Save all buffers."
    },
    {
      "action": ":!python -m json.tool",
      "description": "Pretty format of JSON data."
    },
    {
      "action": "ctrl-x",
      "description": "Open file in new vertical buffer in ctrlp."
    },
    {
      "action": "ctrl-v",
      "description": "Open file in new horizontal buffer in ctrlp."
    },
    {
      "action": "\"*p",
      "description": "Paste from selection clipboard."
    },
    {
      "action": "\"+p",
      "description": "Paste from usual clipboard, ctrl+C."
    },
    {
      "action": "D",
      "description": "Delete from cursor to the end of line."
    },
    {
      "action": "d$",
      "description": "Delete from cursor to the end of line."
    },
    {
      "action": "dw",
      "description": "Delete from cursor to the beginning of the next word."
    }
  ] 

}
